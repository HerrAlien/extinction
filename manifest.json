{
    "manifest_version" : 2,
    "name" : "Extinction-o-Meter",
    "version": "1.2.4",
    "icons" : { 
		"128" : "resources/icon128.png",
		"256" : "resources/icon.png"
    },
    "permissions" : ["<all_urls>", "geolocation"],
    "app" : {
        "background" : {
            "scripts" : ["chromeapp.js"]
        }
    },
    "url_handlers": {
      "handle_data_sharing_secure": {
        "matches": [
          "https://extinction-o-meter.appspot.com/*"
        ],
        "title": "Extinction-O-Meter"
      },
      "handle_data_sharing": {
        "matches": [
          "http://extinction-o-meter.appspot.com/*"
        ],
        "title": "Extinction-O-Meter"
      }
    }
}
